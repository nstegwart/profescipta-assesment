{
  "name": "profescipt-assesment",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "clean": "cd android && ./gradlew clean",
    "pod": "rm -rf ios/Pods ios/Podfile.lock && cd ios && pod install",
    "releaseApk": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk/release",
    "bundleRelease": "cd android && ./gradlew clean && ./gradlew bundleRelease && open app/build/outputs/bundle/release",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "^1.19.8",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "add": "^2.0.6",
    "axios": "0.24.0",
    "expo": "~49.0.15",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "moment": "^2.29.4",
    "querystring": "^0.2.1",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-date-picker": "^4.3.3",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-persist-expire": "^1.1.1",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
